<!DOCTYPE html>
<html>
  <head>
    <title>TempoApi</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <header>
    <%= link_to 'Sign in with Spotify', '/auth/spotify' %>
  </header>

  <body>
    <%= yield %>
  </body>
</html>
